
@{
    Layout = null;
}
@using YiYun.Entity

@model List<JFMX>
<!doctype html>
<html>
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, user-scalable=no" />
    <title>缴费明细</title>
    <script src="~/Scripts/jquery-2.1.1.js"></script>
    <link href="~/Content/jqweui/weui.min.css" rel="stylesheet" type="text/css" media="screen" />
    <link href="~/Content/jqweui/jquery-weui.min.css" rel="stylesheet" type="text/css" media="screen" />
    <script src="~/Scripts/jqweui/jquery-weui.min.js" type="text/javascript"></script>
    <script src="~/Scripts/jquery.cookie.js" type="text/javascript"></script>
    <script src="~/Scripts/jquery.validate.js" type="text/javascript"></script>
    <script src="~/Scripts/MyWechat.validate.js"></script>
    <script src="https://cdn.bootcss.com/zepto/1.2.0/zepto.js" type="text/javascript" charset="utf-8"></script>
    <!--引入微信的两个js-->
    <script type="text/javascript" src="https://res.wx.qq.com/open/js/jweixin-1.0.0.js"></script>
    <!--weui.min.js有版本之分，在低版本的js中有些功能不完善，在高版本会支持-->
    <script src="https://res.wx.qq.com/open/libs/weuijs/1.0.0/weui.min.js"></script>
    <script src="~/Scripts/jqweui/jquery-weui.js"></script>
</head>
<body>
    <ul>
        @foreach (var i in Model)
        {
            <li>
                <div class="weui-form-preview">
                    <div class="weui-form-preview__bd">
                        <div class="weui-form-preview__item">
                            <label class="weui-form-preview__label">缴费单号</label>
                            <span class="weui-form-preview__value">@i.SN</span>
                        </div>
                        <div class="weui-form-preview__item">
                            <label class="weui-form-preview__label">缴费日期</label>
                            <span class="weui-form-preview__value">@i.CreateAt</span>
                        </div>
                        <div class="weui-form-preview__item">
                            <label class="weui-form-preview__label">缴费方式</label>
                            <span class="weui-form-preview__value">@i.WxPayType</span>
                        </div>
                        <div class="weui-form-preview__item">
                            <label class="weui-form-preview__label">付款金额</label>
                            <em class="weui-form-preview__value"><strong>@i.DJSSAmount</strong></em>
                        </div>
                    </div>
                </div>
            </li>
        }
    </ul>
</body>
</html>






