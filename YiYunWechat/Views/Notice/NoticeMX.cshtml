@{
    Layout = null;
}
@using YiYun.Entity

@model Notice
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <title>公告详情</title>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1, user-scalable=no">
    <link href="~/Content/jqweui/jquery-weui.css" rel="stylesheet" />
    <link href="~/Content/jqweui/jquery-weui.min.css" rel="stylesheet" />
    <link href="~/Content/jqweui/weui.css" rel="stylesheet" />
    <link href="~/Content/jqweui/weui.min.css" rel="stylesheet" />
</head>
<body ontouchstart>
    <div class="weui-content">
        <article class="weui-article">
            <h1 style="color: deepskyblue; text-align: center">@Model.NoticeTitle</h1>
            <h1 style="text-align:center; font-size:12px">@Model.CreateName @Model.CreateDate</h1>
            <section>
                <p>@Model.NoticeContent</p>
            </section>
        </article>
    </div>
</body>
</html>
